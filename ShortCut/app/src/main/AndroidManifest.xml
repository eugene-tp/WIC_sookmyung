        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Navi.NaviStraight"></activity>
        <activity android:name=".Navi.NaviStartRunning" />
        <activity android:name=".Navi.NaviStart"></activity>
        <activity android:name=".Setting.SettingMain" />
        <activity android:name=".Main.SearchMapActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="96905ec450d4ef0975a5737dcf909332" />
